"script":"(function () {\r\n\r\n  function hideArcnxtLayout() {\r\n\r\n    const selectors = [\r\n      \"#masthead\",                 // main header\r\n      \".site-header\",\r\n      \".header-layout-1\",\r\n\r\n      \".app-download-header\",      // app download banner\r\n\r\n      \"#colophon\",                 // main footer\r\n      \".site-footer\",\r\n      \".site-info\",\r\n\r\n      \".elementor-location-header\",\r\n      \".elementor-location-footer\"\r\n    ];\r\n\r\n    selectors.forEach(selector => {\r\n      document.querySelectorAll(selector).forEach(el => {\r\n        el.style.display = \"none\";\r\n        el.style.visibility = \"hidden\";\r\n        el.style.height = \"0\";\r\n        el.style.overflow = \"hidden\";\r\n      });\r\n    });\r\n\r\n    // Remove spacing created by header/footer\r\n    document.body.style.marginTop = \"0\";\r\n    document.body.style.paddingTop = \"0\";\r\n    document.body.style.marginBottom = \"0\";\r\n    document.body.style.paddingBottom = \"0\";\r\n  }\r\n\r\n  // Run immediately\r\n  hideArcnxtLayout();\r\n\r\n  // Run after page load\r\n  window.addEventListener(\"load\", hideArcnxtLayout);\r\n\r\n  // Handle dynamically injected elements (Woostify / Elementor / Woo)\r\n  const observer = new MutationObserver(hideArcnxtLayout);\r\n  observer.observe(document.body, {\r\n    childList: true,\r\n    subtree: true\r\n  });\r\n\r\n})();\r\n",
